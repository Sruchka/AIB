
Процедура ПередЗаписью(Отказ)		
	
	ХвостСервиса = "/ws/WebCommonInfoBases.1cws?wsdl";
	
	ЭтотОбъект.СсылкаНаВебСервис = СокрЛП(ЭтотОбъект.СсылкаНаВебСервис);
	
	Отказ = НЕ (
		(Лев(ЭтотОбъект.СсылкаНаВебСервис, 7) = "http://") ИЛИ Лев(ЭтотОбъект.СсылкаНаВебСервис, 8) = "https://"
		И Прав(ЭтотОбъект.СсылкаНаВебСервис, СтрДлина(ХвостСервиса)) = ХвостСервиса
	);
	
	
